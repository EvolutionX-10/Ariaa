{
	"name": "ariaa",
	"version": "1.3.0",
	"description": "A CLI for music lovers",
	"main": "dist/index.js",
	"type": "module",
	"exports": "./dist/index.js",
	"bin": {
		"aria": "./dist/index.js",
		"ariaa": "./dist/index.js"
	},
	"imports": {
		"#utils": "./dist/utils/index.js",
		"#constants": "./dist/constants.js",
		"#lib/*": "./dist/lib/*/index.js",
		"#core": "./dist/core/index.js"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "tsup",
		"watch": "tsup --watch",
		"bump": "cliff-jumper",
		"format": "prettier --write './src/**/*.ts' './README.md'",
		"pretty": "prettier --check './src/**/*.ts' './README.md'",
		"typecheck": "tsc -b src",
		"lint": "eslint --fix --ext ts",
		"postinstall": "husky install",
		"prepack": "yarn build && pinst --disable",
		"postpack": "pinst --enable"
	},
	"keywords": [
		"cli",
		"music",
		"spotify",
		"youtube",
		"song"
	],
	"files": [
		"dist",
		"!dist/*.tsbuildinfo",
		"!dist/**/*.js.map",
		"!dist/**/*.d.ts",
		"!dist/**/*.d.ts.map"
	],
	"author": "EvolutionX-10",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/EvolutionX-10/Ariaa.git"
	},
	"engines": {
		"node": ">=v16.6.0",
		"npm": ">=7.0.0"
	},
	"bugs": {
		"url": "https://github.com/EvolutionX-10/Ariaa/issues"
	},
	"homepage": "https://github.com/EvolutionX-10/Ariaa",
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "yarn@3.5.0",
	"devDependencies": {
		"@favware/cliff-jumper": "2.0.0",
		"@favware/npm-deprecate": "1.0.7",
		"@sapphire/eslint-config": "4.4.1",
		"@sapphire/prettier-config": "1.4.5",
		"@sapphire/ts-config": "4.0.0",
		"@swc/core": "1.3.51",
		"@types/cli-progress": "3.11.0",
		"@types/fluent-ffmpeg": "2.1.21",
		"@types/inquirer": "9.0.3",
		"@types/node": "18.15.11",
		"@typescript-eslint/eslint-plugin": "5.59.0",
		"@typescript-eslint/parser": "5.59.0",
		"eslint": "8.38.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-prettier": "4.2.1",
		"husky": "8.0.3",
		"pinst": "3.0.0",
		"prettier": "2.8.7",
		"pretty-quick": "3.1.3",
		"tsup": "6.7.0",
		"typescript": "4.9.5"
	},
	"dependencies": {
		"@ffmpeg-installer/ffmpeg": "^1.1.0",
		"cli-progress": "3.12.0",
		"colorette": "2.0.20",
		"commander": "10.0.1",
		"fluent-ffmpeg": "2.1.2",
		"inquirer": "9.1.5",
		"inquirer-select-directory": "1.2.0",
		"sanitize-filename": "1.6.3",
		"undici": "5.21.2",
		"youtube-sr": "4.3.4",
		"ytdl-core": "4.11.4"
	},
	"prettier": "@sapphire/prettier-config"
}
